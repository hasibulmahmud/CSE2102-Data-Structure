#include <bits/stdc++.h>
using namespace std;

/// Directed and Unweighted Graph.
/// Adjacency Matrix representation
int main() {
    int vertex, edges;
    cin >> vertex >> edges;

    // Initialize matrix with 0 (no edge)
    vector<vector<int>> AdjMat(vertex, vector<int>(vertex, 0));

    int u, v;
    for (int i = 0; i < edges; i++) {
        cin >> u >> v;      // edge from u â†’ v
        AdjMat[u][v] = 1;   // directed, so only u->v is marked
    }

    // Display
    for (int i = 0; i < vertex; i++) {
        for (int j = 0; j < vertex; j++) {
            cout << AdjMat[i][j] << " ";
        }
        cout << endl;
    }
}
